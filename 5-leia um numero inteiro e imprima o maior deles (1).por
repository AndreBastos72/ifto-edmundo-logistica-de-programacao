programa
{
  funcao inicio()
  {
    inteiro num1, num2

    escreva ("Escreva o 1 numero: ")
      leia (num1)
    escreva ("Escreva o 2 numero: ")
      leia (num2)

    se (num1 > num2)
    {
      escreva ("O numero ", (num1) , " é o maior")
    }
    senao se (num1 < num2)
    {
        escreva ("O numero ", (num2) , " é o maior")
    }
  }
}
