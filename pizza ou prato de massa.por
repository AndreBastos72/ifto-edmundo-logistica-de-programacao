programa {
  funcao inicio() {
    
    cadeia escolher
    caracter pizza
    

    escreva("Para o jantar, você prefere e comer uma pizza ou um prato de massa?")
      leia(escolher)

    se (escolher == "pizza" ou pizza == "prato de massa"){
      escreva(" Eu prefiro comer uma pizza! ")}

    senao {
    escreva(" Comer um prato de massa! ")
   }
  }
}
